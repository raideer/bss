{
  "name": "SSPlus",
  "version": "1.1.0",
  "description": "SS.com enhancements",
  "icons": {
    "16": "icons/ssplus16.png",
    "48": "icons/ssplus48.png",
    "128": "icons/ssplus128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.ss.com/*",
        "https://*.ss.lv/*"
      ],
      "run_at": "document_start",
      "js": [
        "dist/ssplus.js"
      ],
      "css": [
        "dist/ssplus.css"
      ]
    }
  ],
  "manifest_version": 2
}